    "description": "Write me to read me.",
    "type": "application",
    "require": {
        "symfony/yaml": "^3.0",
        "aura/cli": "^2.1",
        "nette/finder": "^2.3",
        "roave/better-reflection": "^1.0",
        "nochso/omni": "^0.3.0"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "^1.11",
        "phpunit/phpunit": "^5.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Marcel Voigt",
            "email": "mv@noch.so"
        }
    ],
    "autoload": {
